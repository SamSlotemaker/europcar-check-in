<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %>

<body class="reservations">

    <%- include('../partials/header.ejs') %>

    <h1>Reserveringen</h1>
    <ul class="reservations__cars">
        <% reservations.forEach(car => { %>
        <li>
            <a href="/cars/<%= car.id %>">
                <img src="<%= car.imgUrl %>" alt="">
                <div>
                    <h2><%= car.car %> <%= car.model %></h2>
                </div>
                <strong class="check-in__status">Status: <span><%= car.checkedIn %></span></strong>
            </a>
        </li>
        <% }) %>
    </ul>

</body>

</html>